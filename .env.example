# Fanvue OAuth (from the Dev Portal)
FANVUE_CLIENT_ID=your_client_id
FANVUE_CLIENT_SECRET=your_client_secret
FANVUE_REDIRECT_URI=http://localhost:8000/callback

# Webhooks (Still separate from OAuth)
FANVUE_WEBHOOK_SECRET=whsec_your_signing_secret

# OpenRouter (Keys > Create New Key)
OPENROUTER_API_KEY=sk-or-v1_your_openrouter_key

# Supabase (Settings > API)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=sb_publishable_your_supabase_key

# Supabase DB (Settings > Database > Connection String > Python)
SUPABASE_DB_URL=postgresql://postgres:your_password@db.your-project.supabase.co:5432/postgres
